{
  "name": "moss-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "gen": "node scripts/gen-component.mjs",
    "preview": "vite preview",
    "dev": "vitepress dev",
    "build": "vitepress build",
    "play": "npm run dev --prefix play",
    "docs:dev": "pnpm --filter @moss-ui/docs dev",
    "docs:build": "pnpm --filter @moss-ui/docs build",
    "docs:preview": "pnpm -C docs vitepress preview",
    "release": "bumpp package.json packages/components/package.json && pnpm build && pnpm --filter @moss-ui/components publish",
    "build:component": "pnpm --filter @moss-ui/components build"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "bumpp": "^10.3.2",
    "enquirer": "^2.4.1",
    "kolorist": "^1.8.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.1.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
